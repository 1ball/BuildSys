VERSION=4.8.3
PKG_LOCATION="http://releases.qt-project.org/qt4/source/qt-everywhere-opensource-src-$VERSION.tar.gz"
TOOL_DEPS=pkg-config
DEPS=(sqlite dbus)
CONFIGFLAGS=(
			"-prefix $PREFIX"
            -release
            "-nomake examples"
            "-nomake demos"
            -system-sqlite
            -no-nis
            -opensource
            -confirm-license
			"-embedded x86"
			)
MAKEFLAGS=$MAKEJOBS
